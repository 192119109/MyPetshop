                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        t1.id_pengurangan, t1.id_barang, t2.nama_barang, t1.id_pembelian, t3.id_supplier, t4.nama AS nama_supplier, t1.tglPengurangan, t1.qtyAwal, t1.qtyAkhir, t1.jlhPengurangan, t1.jlhPengurangan * t5.[harga/pcs] AS jlhKerugian, 
                         t1.Keterangan
FROM            Pengurangan_Stock AS t1 INNER JOIN
                         Barang AS t2 ON t1.id_barang = t2.id_barang INNER JOIN
                         Pembelian AS t3 ON t1.id_pembelian = t3.id_pembelian INNER JOIN
                         Suppliers AS t4 ON t3.id_supplier = t4.id_supplier INNER JOIN
                         Pembelian_Detail AS t5 ON t1.id_pembelian = t5.id_pembelian AND t1.id_barang = t5.id_barang
WHERE        (t1.tglPengurangan BETWEEN @startDate AND @endDate)
ORDER BY t1.tglPengurangan DESC</CommandText>
